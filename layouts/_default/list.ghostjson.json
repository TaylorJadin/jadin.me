{{- $pages := where .Site.RegularPages "Title" "!=" "" -}}
{{- $counter := 0 -}}
{
  "meta": {
    "exported_on": {{ now.Unix }},
    "version": "2.14.0"
  },
  "data": {
    "posts": [
      {{- range $index, $element := $pages -}}
      {{- $counter = add $counter 1 -}}
      {{- if $index }},{{ end -}}
      {
        "id": {{ $counter }},
        "title": {{ $element.Title | jsonify }},
        "slug": {{ (path.Base $element.File.Dir) | jsonify }},
        "html": {{ $element.Content | jsonify }},
        "featured_iamge": null,
        "feature_image_alt": null,
        "feature_image_caption": null,
        "featured": 0,
        "page": 0,
        "status": "published",
        "published_at": {{ $element.PublishDate.Unix }},
        "published_by": 1,
        "meta_title": null,
        "meta_description": null,
        "email_only": false,
        "author_id": 1,
        "created_at": {{ $element.Date.Unix }},
        "created_by": 1,
        "updated_at": {{ $element.Lastmod.Unix }},
        "updated_by": 1
      }
      {{- end -}}
    ]
  }
}
